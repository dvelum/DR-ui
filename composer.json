{
  "name": "dvelum/dr-ui",
  "version": "0.3",
  "type": "library",
  "description": "UI for DVelum Data Record",
  "keywords": ["php"],
  "homepage": "https://github.com/dvelum/drui",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Kirill Yegorov",
      "role": "Founder, Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/dvelum/dr-ui/issues",
    "source": "https://github.com/dvelum/dr-ui"
  },
  "require": {
    "php": "~7.4",
    "ext-json": "*",
    "composer/installers": "~1.9",
    "dvelum/dr": "~1.2",
    "dvelum/extjs": "^7.0",
    "laminas/laminas-diactoros": "^2.5",
    "laminas/laminas-http": "^2.14",
    "laminas/laminas-httphandlerrunner": "^1.3",
    "nette/neon": "^3.2",
    "oomphinc/composer-installers-extender": "^2.0",
    "matthiasmullie/minify": "~1.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "bin": [
    "drui"
  ],
  "autoload": {
    "psr-4": {
      "Dvelum\\DR\\Ui\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dvelum\\DR\\Ui\\UnitTest\\": "tests/unit/"
    }
  },
  "extra": {
    "installer-types": [
      "js-library"
    ],
    "installer-paths": {
      "public/js/lib/{$name}": [
        "type:js-library"
      ]
    }
  }
}
